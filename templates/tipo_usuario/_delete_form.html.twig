<form method="post" action="{{ path('tipo_usuario_delete', {'idtipoUsuario': tipo_usuario.idtipoUsuario}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ tipo_usuario.idtipoUsuario) }}">
    <button class="btn">Delete</button>
</form>
