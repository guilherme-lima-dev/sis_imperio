<form method="post" action="{{ path('estabelecimento_delete', {'idestabelecimento': estabelecimento.idestabelecimento}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ estabelecimento.idestabelecimento) }}">
    <button class="btn">Delete</button>
</form>
